<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <template id="_assets_primary_variables_inh_fahad_theme_backend" inherit_id="web._assets_primary_variables">
            <xpath expr="." position="inside">
                <link rel="stylesheet" type="text/scss" href="/fahad_theme_backend/static/src/scss/colors.scss"/>
            </xpath>
        </template>

	<template id="_assets_utils" name="Mixins" inherit_id="web._assets_utils" priority="15">
		<xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/fahad_theme_backend/static/src/scss/mixins.scss"/>
		</xpath>
    </template>
    
	<template id="_assets_backend_helpers" name="Variables" inherit_id="web._assets_backend_helpers" priority="15">
        <xpath expr="//link[@href='/web/static/src/scss/bootstrap_overridden.scss']" position="after">
            <link rel="stylesheet" type="text/scss" href="/fahad_theme_backend/static/src/scss/variables.scss"/>
        </xpath>
    </template>
    
	<template id="assets_common" name="Common Assets" inherit_id="web_editor.assets_common">
		<xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/fahad_theme_backend/static/src/scss/fonts.scss"/>
		</xpath>

	</template>

   
</odoo>
